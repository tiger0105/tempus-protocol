  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "decimal.js": "^10.3.1",
    "dotenv": "^10.0.0",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.4.1",
    "hardhat": "^2.2.1",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-deploy": "^0.8.9",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "prompt": "^1.2.0",
    "prompts": "^2.4.2",
    "solhint": "^3.3.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.0.0",
    "typechain": "^5.1.1",
    "typescript": "^4.2.4",
    "yahoo-finance": "^0.3.7"
  },
  "scripts": {
    "build": "npx hardhat compile",
    "test": "cross-env yarn build && npx mocha --require hardhat/register --timeout 10000 --recursive --parallel --extension ts",
    "test:integration:lido": "cross-env yarn build && cross-env INTEGRATION=1 ONLY_POOL=Lido ETH_NODE_URI_MAINNET=https://eth-mainnet.alchemyapi.io/v2/yInmJCdHvCzXe4qAxfPMDFgrbYIGsUhJ HARDHAT_FORK=mainnet HARDHAT_FORK_NUMBER=13533408 npx mocha --require hardhat/register --timeout 120000 --recursive --parallel --extension ts",
    "test:integration": "cross-env yarn build && ETH_NODE_URI_MAINNET=https://eth-mainnet.alchemyapi.io/v2/yInmJCdHvCzXe4qAxfPMDFgrbYIGsUhJ HARDHAT_FORK=mainnet HARDHAT_FORK_NUMBER=13281002 npx mocha --timeout 120000 --require hardhat/register --recursive --parallel --extension ts integration-tests",
    "coverage": "npx hardhat coverage --testfiles \"test/**/*.ts\"",
    "coverage:integration": "cross-env HARDHAT_FORK=mainnet HARDHAT_FORK_NUMBER=13281002 npx hardhat coverage --testfiles \"integration-tests/**/*.ts\"",
    "solhint": "solhint \"contracts/**/*.sol\"",
    "lint": "prettier --write \"contracts/**/*.sol\"",
    "lint-check": "prettier --check \"contracts/**/*.sol\"",
    "start-local": "npx hardhat node",
    "start-local:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat node",
    "deploy-local:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat run scripts/deploy.local.fork.ts --network localhost",
    "deposit-local:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat run scripts/deposit.local.fork.ts --network localhost",
    "deploy-ci:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat run scripts/deploy.local.fork.ts",
    "deploy-local": "npx hardhat run scripts/deploy.local.ts --network localhost",
    "deploy-ci": "npx hardhat run scripts/deploy.local.ts",
    "deploy:controller": "npx hardhat run scripts/deploy.controller.ts",
    "deploy:stats": "npx hardhat run scripts/deploy.stats.ts",
    "deploy:vault": "npx hardhat run scripts/deploy.vault.ts",
    "deploy:pool:lido": "npx hardhat run scripts/deploy.pool.lido.ts",
    "verify:pool:lido": "npx hardhat run scripts/verify.pool.lido.ts",
    "increase-time": "npx hardhat run scripts/increaseTime.local.ts --network localhost",
    "mythx-install": "pip install mythx-cli",
    "mythx-amm": "mythx --ci --yes analyze --wait --solc-version 0.7.6 contracts/amm/TempusAMM.sol contracts/amm/TempusAMMUserDataHelpers.sol",
    "mythx-pool": "mythx --ci --yes analyze --wait --solc-version 0.8.10 contracts/pools/AaveTempusPool.sol contracts/pools/CompoundTempusPool.sol contracts/pools/LidoTempusPool.sol",
    "mythx-misc": "mythx --ci --yes analyze --wait --solc-version 0.8.10 contracts/stats/Stats.sol contracts/token/TempusToken.sol contracts/token/vesting/ERC20Vesting.sol",
    "slither": "rm -rf contracts/amm contracts/TempusController.sol && slither ."
  },
  "dependencies": {
    "@balancer-labs/v2-asset-manager-utils": "https://gitpkg.now.sh/balancer-labs/balancer-v2-monorepo/pkg/asset-manager-utils?ccb2d594a893cd4b7ce79698e373cb100b28aaa2",
    "@balancer-labs/v2-pool-stable": "https://gitpkg.now.sh/balancer-labs/balancer-v2-monorepo/pkg/pool-stable?ccb2d594a893cd4b7ce79698e373cb100b28aaa2",
    "@balancer-labs/v2-pool-utils": "https://gitpkg.now.sh/balancer-labs/balancer-v2-monorepo/pkg/pool-utils?ccb2d594a893cd4b7ce79698e373cb100b28aaa2",
    "@balancer-labs/v2-solidity-utils": "https://gitpkg.now.sh/balancer-labs/balancer-v2-monorepo/pkg/solidity-utils?ccb2d594a893cd4b7ce79698e373cb100b28aaa2",
    "@balancer-labs/v2-vault": "https://gitpkg.now.sh/balancer-labs/balancer-v2-monorepo/pkg/vault?ccb2d594a893cd4b7ce79698e373cb100b28aaa2",
    "@openzeppelin/contracts": "^4.1.0"
  }
}
